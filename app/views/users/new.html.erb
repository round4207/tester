<%= content_for :header do %>
	Tweeter
<% end %>

<h1>Create a new user</h1>
<%= form_for @user do |f| %>
	<%= f.label :username %>
	<%= f.text_field :username %>
	<br />
	<%= f.label :password %>
	<%= f.password_field :password %>
	<br />
	<%= f.submit 'Save' %>
<% end %>
<!--automatic na punta sa fields of model-->

<%= content_for :footer do %>
	Java nice day
<% end %>
